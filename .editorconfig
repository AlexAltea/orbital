# EditorConfig
# http://editorconfig.org

root = true

[**.{c,cpp,h}]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true

[**.{py}]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true

[**.{html,css}]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true

[**.{js}]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true
